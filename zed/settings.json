{
  "agent_ui_font_size": 16.0,
  "agent": {
    "default_model": {
      "provider": "copilot_chat",
      "model": "claude-sonnet-4",
    },
    "model_parameters": [],
  },
  "colorize_brackets": true,
  "file_scan_exclusions": [
    "node_modules",
    "dist",
    ".claude",
    ".turbo",
    "build",
    ".git",
    "coverage",
    ".angular",
    ".gradle",
    "target",
  ],
  "code_actions_on_format": {
    "source.organizeImports": true,
    "source.removeUnusedImports": true,
    "source.sortImports": true,
  },
  "features": {
    "edit_prediction_provider": "copilot",
  },
  "base_keymap": "VSCode",
  "buffer_font_size": 16.0,
  "collaboration_panel": {
    "button": false,
  },
  "confirm_quit": true,
  "current_line_highlight": "gutter",
  "edit_predictions": {
    "mode": "subtle",
  },
  "file_scan_inclusions": ["**/.env"],
  "buffer_font_features": {
    "calt": false,
  },
  "format_on_save": "on",
  "notification_panel": {
    "button": false,
  },
  "outline_panel": {
    "button": false,
  },
  "project_panel": {
    "hide_gitignore": false,
    "dock": "right",
  },
  "search": {
    "button": false,
  },
  "tab_bar": {
    "show_nav_history_buttons": false,
    "show_tab_bar_buttons": false,
  },
  "telemetry": {
    "diagnostics": true,
    "metrics": true,
  },
  "terminal": {
    "font_family": "JetBrainsMono Nerd Font Mono",
    "toolbar": {
      "breadcrumbs": false,
    },
  },
  "theme": {
    "dark": "Zedokai",
    "light": "Ros√© Pine Dawn",
    "mode": "light",
  },
  "toolbar": {
    "breadcrumbs": false,
    "quick_actions": false,
  },
  "ui_font_size": 16.0,
  "vim_mode": true,
  "vim": {
    "use_system_clipboard": "never",
    "toggle_relative_line_numbers": true,
  },
  "which_key": {
    "enabled": true,
  },
}
