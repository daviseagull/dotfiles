{
  "agent": {
    "default_model": {
      "provider": "copilot_chat",
      "model": "gpt-5-mini"
    },
    "default_profile": "ask",
    "always_allow_tool_actions": true,
    "dock": "left",
    "play_sound_when_agent_done": true
  },
  "base_keymap": "VSCode",
  "buffer_font_size": 14,
  "code_actions_on_format": {
    "source.organizeImports": true,
    "source.removeUnusedImports": true,
    "source.sortImports": true
  },
  "collaboration_panel": {
    "button": false
  },
  "confirm_quit": true,
  "current_line_highlight": "gutter",
  "edit_predictions": {
    "mode": "subtle"
  },
  "file_scan_inclusions": ["**/.env"],
  "features": {
    "edit_prediction_provider": "copilot"
  },
  "buffer_font_features": {
    "calt": false
  },
  "file_scan_exclusions": [
    "**/.gradle",
    "**/.idea",
    "**/build",
    "**/build-cache",
    "**/bin",
    "**/.classpath",
    "**/.project",
    "**/.claude",
    "**/.ropeproject",
    "**/.ruff_cache",
    "**/.git",
    "**/.settings",
    "**/.turbo",
    "**/.pytest_cache",
    "**/.DS_Store",
    "**/.vscode",
    "**/__pycache__",
    "**/coverage",
    "**/dist",
    "**/.angular",
    "**/target",
    "**/.terraform",
    "**/node_modules",
    "**/.aws-sam",
    "**/tmp"
  ],
  "format_on_save": "on",
  "git_panel": {
    "button": false
  },
  "icon_theme": {
    "dark": "Colored Zed Icons Theme Dark",
    "light": "Colored Zed Icons Theme Light",
    "mode": "system"
  },
  "notification_panel": {
    "button": false
  },
  "outline_panel": {
    "button": false
  },
  "project_panel": {
    "dock": "right"
    // "auto_fold_dirs": false
  },
  "search": {
    "button": false
  },
  "tab_bar": {
    "show_nav_history_buttons": false,
    "show_tab_bar_buttons": false
  },
  "telemetry": {
    "diagnostics": true,
    "metrics": true
  },
  "terminal": {
    "font_family": "JetBrainsMono Nerd Font Mono",
    "toolbar": {
      "breadcrumbs": false
    }
  },
  "theme": {
    "dark": "Ayu Mirage",
    "light": "Ayu Light",
    "mode": "system"
  },
  "toolbar": {
    "breadcrumbs": false,
    "quick_actions": false
  },
  "ui_font_size": 16
}
